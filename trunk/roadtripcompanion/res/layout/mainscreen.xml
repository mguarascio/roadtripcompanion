<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
android:id="@+id/LinearLayout01" 
android:orientation="vertical"
android:layout_width="fill_parent" 
android:layout_height="fill_parent" 
xmlns:android="http://schemas.android.com/apk/res/android">

<TextView 
android:id="@+id/TextView01" 
android:layout_width="fill_parent" 
android:layout_height="wrap_content" android:text="RoadTripCompanion"></TextView>

<ImageButton 
android:layout_width="wrap_content" 
android:layout_height="wrap_content" 
android:id="@+id/createTripButton"></ImageButton>

<ImageButton 
android:layout_width="wrap_content" 
android:layout_height="wrap_content" 
android:id="@+id/viewTripButton"></ImageButton>

<ImageButton 
android:layout_width="wrap_content" 
android:layout_height="wrap_content" 
android:id="@+id/tweetButton"></ImageButton>

<ImageButton 
android:layout_width="wrap_content" 
android:layout_height="wrap_content" 
android:id="@+id/takePhotoButton"></ImageButton>

<ImageButton 
android:layout_width="wrap_content"
android:layout_height="wrap_content" 
android:id="@+id/handleSettingsButton"></ImageButton>

</LinearLayout>
